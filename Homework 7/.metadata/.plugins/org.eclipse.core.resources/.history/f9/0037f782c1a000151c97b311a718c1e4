
public class Teacher extends Person {
	
	private String subject;
	
	public void setSubject(String subject) {
		this.subject = subject;
	}
	
	public String getSubject() {
		return subject;
	}
	
	public Teacher() {
		super();
	}
	
	public Teacher(String name, String familyName, int workAge, String subject) {
		super(name, familyName, workAge);
		setSubject(subject);
	}
	
	public void homework(int hw) {
		System.out.println(hw);
	}
	
	public String goodDay() {
		String goodDay = "Good Day!";
		return goodDay;
	}
	
	public String gooBye() {
		String goodBye = "Good Bye!";
		return goodBye;
	}
}
