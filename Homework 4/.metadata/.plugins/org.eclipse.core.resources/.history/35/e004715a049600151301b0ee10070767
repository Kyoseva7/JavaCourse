
public class Zad6 {

	public static void main(String[] args) {
		int[] arr = new int[] {3, 7, 2, 8, 12, 1, 65, 3, 34, 2, 14};
		int tmp;
			for (int i = 1; i < arr.length; i++) {
			if (arr[i] < arr[i - 1]) {
				tmp = arr[i];
				arr[i] = arr[i -1];
				arr[i - 1] = tmp;
			}
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i] + " ");
		}
	}

}
