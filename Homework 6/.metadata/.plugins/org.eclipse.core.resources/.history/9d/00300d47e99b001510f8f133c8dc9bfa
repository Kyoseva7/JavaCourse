
public class Person {
	
	private String name;
	private byte age;
	private Long egn;
	
	public void setName(String name) {
		if(name != null) {
			this.name = name;
		}
	}
	
	public void setAge(byte age) {
		if(age >= 0){
			this.age = age;
		}
	}
	
	public void setEgn(Long egn) {
		if(Long.SIZE == 10) {
			this.egn = egn;
		}
	}
	
	public static void print(String name, byte age, Long egn){
		System.out.println("name: " + name);
		System.out.println("age: " + age);
		System.out.println("egn: " + egn);
	}
		
	}
