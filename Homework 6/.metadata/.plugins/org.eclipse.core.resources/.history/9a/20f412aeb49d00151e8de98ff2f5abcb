
public class SUV extends Car {
	
	boolean highGo;
	
	public SUV() {
		super();
		this.highGo = false;
	}
	
	public SUV(double price, boolean highGo) {
		super(price);
		setHighGo();
	}
	
	public void setHighGo() {
		if(true) {
			this.highGo = true;
		}
	}
	
	public boolean getHighGo() {
		return highGo;
	}
}
