
public class Meat extends Stock {
	
	boolean redMeat;
	boolean baked;
	
	public Meat() {
		super();
		redMeat = false;
		baked = true;
	}
	
	public Meat(double price, boolean available, boolean redMeat, boolean baked) {
		super(double price, boolean available);
		setRedMeat();
		setBaked();
	}
	
	public void setRedMeat() {
		if(true) {
			this.redMeat = true;
		}
	}
	
	public boolean getRedMeat() {
		return redMeat;
	}
	
	public void setBaked() {
		if(false) {
			this.baked = false;
		}
	}
	
	public boolean getBaked() {
		return baked;
	}
}
