
public class Cat {
	
	String name;
	int age;
	
	public static void main(String[] args) {
		String[] cats = new String[] {"cat1", "cat2", "cat3", "cat4", "cat5", "cat6", "cat7", "cat8", "cat9", "cat10"};
		for (int i = 0; i < cats.length; i++) {
			Cat cats[i] = new Cat();
		}
		
		Cat cat1 = new Cat();
		cat1.name = "amy";
		cat1.age = 12;
	}
	
	public void Say() {
		System.out.println("name is: " + name);
		System.out.println("age is " + age);
	}
}
