import java.util.Scanner;

public class Ex2 {

	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		System.out.println("Vuvedete chislo: ");
		int N = input.nextInt();
		System.out.println("Vuvedete chislo: ");
		int M = input.nextInt();
		for (int i = 0; i < M; i++) {
			N *= N;
		}
		input.close();
	}

}
